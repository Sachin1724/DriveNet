{
  "expo": {
    "name": "DriveNet",
    "slug": "drivenet",
    "scheme": "drivenet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e0e0e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sachin1724.drivenet"
    },
    "android": {
      "package": "com.sachin1724.drivenet",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0e0e0e"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "310846ab-f67b-4dea-b910-3fa9adfdace5"
      }
    },
    "owner": "sachinmallick"
  }
}
